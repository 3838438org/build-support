<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
		<string>org.x.font_cache</string>
	<key>ProgramArguments</key>
		<array>
			<string>/usr/X11/bin/font_cache.sh</string>
			<string>-s</string>
		</array>
	<key>UserName</key>
		<string>root</string>
	<key>RunAtLoad</key>
		<true/>
	<key>Nice</key>
		<integer>15</integer>
	<key>LowPriorityIO</key>
		<true/>
</dict>
</plist>
